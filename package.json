{
  "name": "aurora-seo",
  "version": "1.0.2",
  "description": "The fastest way to add SEO to your Next.js project. Automatically generates sitemaps, robots.txt, metadata, and Google Search Console verification.",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "types": "dist/index.d.ts",
  "bin": {
    "aurora-seo": "dist/src/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CONTRIBUTING.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "echo \"Tests will be added in future versions\" && exit 0",
    "lint": "echo \"Linting will be added in future versions\" && exit 0",
    "prepublishOnly": "npm run build",
    "release": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "keywords": [
    "nextjs",
    "seo",
    "sitemap",
    "robots",
    "metadata",
    "google-search-console",
    "cli",
    "automation",
    "app-router",
    "pages-router",
    "typescript"
  ],
  "author": {
    "name": "Aurora Software Labs",
    "email": "contact@aurorasoftwarelabs.io",
    "url": "https://aurorasoftwarelabs.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aurorasoftwarelabs/aurora-seo.git"
  },
  "bugs": {
    "url": "https://github.com/aurorasoftwarelabs/aurora-seo/issues"
  },
  "homepage": "https://github.com/aurorasoftwarelabs/aurora-seo#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "peerDependencies": {
    "next": ">=11.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "inquirer": "^8.2.6"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.10",
    "@types/node": "^20.8.0",
    "typescript": "^5.2.2"
  }
}
